<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Asset Marketplace Table" max_length="40" name="x_sthc_asset_marke_asset_marketplace_table" type="collection">
        <element label="Description" max_length="1000" name="description" type="html"/>
        <element label="Image" max_length="40" name="image" type="user_image"/>
        <element label="Manufacture" max_length="40" name="manufacture" type="string"/>
        <element label="Model" max_length="40" name="model" type="string"/>
        <element label="Quantity" max_length="40" name="quantity" type="integer"/>
        <element display="true" label="Short Description" max_length="140" name="short_description" type="string"/>
        <element choice="3" label="State" mandatory="true" max_length="40" name="state" type="string">
            <choice>
                <element label="Active" value="1"/>
                <element label="Inactive" value="2"/>
            </choice>
        </element>
    </element>
</database>
